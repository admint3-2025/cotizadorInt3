{
  "name": "integrational-cotizaciones",
  "version": "1.0.0",
  "description": "Sistema de cotizaciones Integrational",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "node --watch backend/server.js",
